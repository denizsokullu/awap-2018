html
  head
    include includes/header.pug
  body
    include includes/structure.pug
      div.content
        .row.leaderboard-outer
          .col-xs-12.col-md-6.col-md-offset-3.leaderboard
            .header Nodes Captured
            each data in allScores
              if data.highlight
                .block.highlighted= data.score
              else
                .block= data.score
